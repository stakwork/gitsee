{
  "name": "gitsee",
  "version": "1.0.16",
  "description": "Git repository visualizer using D3.js graph visualization",
  "main": "dist/client/index.js",
  "types": "dist/client/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "npm run build && node dev-server.js",
    "lint": "eslint client server --ext .js,.ts",
    "format": "prettier --write client/**/*.ts server/**/*.ts",
    "test-agent": "node test-agent.js",
    "test-mcp": "tsx server/github/test-mcp.ts",
    "test-github-agent": "tsx server/agent/test-github-agent.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "git",
    "visualization",
    "d3",
    "graph",
    "repository"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "3.0.0-beta.66",
    "@octokit/rest": "^20.0.0",
    "ai": "6.0.0-beta.124",
    "aieo": "0.1.27",
    "d3": "^7.8.5",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "dotenv": "^17.2.2",
    "isomorphic-git": "^1.25.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/node": "^20.19.13",
    "eslint": "^8.57.0",
    "prettier": "^3.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "screenshot.png"
  ]
}
